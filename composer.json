    "keywords": ["nette", "php", "cli"],
    "type": "library",
    "license": ["MIT", "BSD-3-Clause", "GPL-2.0", "GPL-3.0"],
    "autoload": {
        "classmap": ["src"]
    },
    "scripts": {
        "post-update-cmd": "Spaceboy\\NetteCli\\Helper::postInstall",
        "post-install-cmd": "Spaceboy\\NetteCli\\Helper::postInstall"
